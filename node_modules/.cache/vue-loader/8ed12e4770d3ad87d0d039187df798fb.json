ut-2\">\n        <b-form-input\n          id=\"input-2\"\n          v-model=\"form.name\"\n          placeholder=\"Escriu el teu nom\"\n          required\n        ></b-form-input>\n      </b-form-group>\n\n      <b-form-group id=\"input-group-3\" label=\"Raó de contacte:\" label-for=\"input-3\">\n        <b-form-select\n          id=\"input-3\"\n          v-model=\"form.food\"\n          :options=\"foods\"\n          required\n        ></b-form-select>\n      </b-form-group>\n\n      <b-button class=\"boto\" type=\"submit\" variant=\"primary\">Enviar</b-button>\n    </b-form>\n    </section>\n  </div>\n</template>\n\n<script>\n  export default {\n    data() {\n      return {\n        form: {\n          email: '',\n          name: '',\n          food: null,\n          checked: []\n        },\n        foods: [{ text: 'Selecciona una opció', value: null }, 'Dubte sobre la carrera', 'Aportació al Blog', 'Problema Tècnic', 'Altres'],\n        show: true\n      }\n    },\n    methods: {\n      onSubmit(event) {\n        event.preventDefault()\n        alert(JSON.stringify(this.form))\n      },\n      onReset(event) {\n        event.preventDefault()\n        // Reset our form values\n        this.form.email = ''\n        this.form.name = ''\n        this.form.food = null\n        this.form.checked = []\n        // Trick to reset/clear native browser form validation state\n        this.show = false\n        this.$nextTick(() => {\n          this.show = true\n        })\n      }\n    }\n  }\n</script>\n\n<style>\n    .formulari{\n        margin-block-start: 90px;\n        margin-block-end:19%;\n    }\n    .part2{\n        text-align: left!important;\n        padding-left: 15px;\n        padding-right: 15px;\n        padding-top: 10px;\n    }\n    .boto{\n        background-color: #830051!important;\n        border-color: #830051!important;\n        margin-top: 10px;\n    }\n    \n</style>"]}]}